@model List<MvcOnlineTradingAutomation.Models.Entities.CargoTracking>
@{
    ViewBag.Title = "CargoTracking";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Kargo Hareketleri</h2>
<br />
<input type="hidden" name="TrackingCode" readonly/>
<table class="table table-bordered">
    <tr>
        <th>Açıklama</th>
        <th>Tarih</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Description</td>
            <td>@x.Date</td>
        </tr>
    }
</table>
